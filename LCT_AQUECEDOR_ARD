int transistorPin = 9; // Pino conectado à base do TIP120

void setup() {
  pinMode(transistorPin, OUTPUT);
}

void loop() {
  analogWrite(transistorPin, 128); // Escreve um valor PWM (50% de ciclo de trabalho)
  delay(10000); // Mantém o aquecimento por 10 segundos
  analogWrite(transistorPin, 0);  // Desliga o aquecimento
  delay(10000); // Aguarda 10 segundos
}